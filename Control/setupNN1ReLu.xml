<?xml version="1.0" encoding="UTF-8"?>
<setup>
  <data>
  	<inputSize>4</inputSize>
  	<outputSize>6</outputSize>
  	<noise>no</noise>
  </data>
  <regression>
    <NeuralNet>
      <inputLayer>
		<type>linear</type>
      </inputLayer>
      <hiddenLayers>
        <hiddenLayer>
        	<type>relu</type>
        	<size>10</size>
        </hiddenLayer>
      </hiddenLayers>
      <outputLayer>
		<type>relu</type>
      </outputLayer>
      <bias>yes</bias>
      <learningrate>0.001</learningrate>
      <momentum>0</momentum>
    </NeuralNet>
    <thetaFile>NN1</thetaFile>
    <resultPath>~/stage/ArmModelPython/Data/NN/</resultPath>
  </regression>
  <costFunction>
    <gamma>0.01</gamma>
    <rho>3000</rho>
    <upsilon>1</upsilon>
  </costFunction>
  <CMAES>
    <sigma>0.01</sigma>
    <maxIteration>50000</maxIteration>
    <popsize>30</popsize>
    <numberRepetition>1</numberRepetition>
    <resultPath>~/stage/ArmModelPython/Data/CMAESReLu/</resultPath>
  </CMAES>
  <target>
    <sizes>
      <size>0.005</size>
      <size>0.01</size>
      <size>0.02</size>
      <size>0.04</size>
    </sizes>
    <coordinate>
      <x>0.0</x>
      <y>0.6175</y>
    </coordinate>
  </target>
  <trajectory>
    <initialPosition>PosCircu28</initialPosition>
    <maxStep>300</maxStep>
    <timeStep>0.002</timeStep>
  </trajectory>
  <kalmanFilter>1</kalmanFilter>
  <plot>
    <period>10</period>
  </plot>
</setup>
