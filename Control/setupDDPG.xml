<?xml version="1.0" encoding="UTF-8"?>
<setup>
  <data>
  	<inputSize>4</inputSize>
  	<outputSize>6</outputSize>
  	<noise>yes</noise>
  </data>
  <regression>
    <NeuralNet>
      <inputLayer>
		<type>linear</type>
      </inputLayer>
      <hiddenLayers>
        <hiddenLayer>
        	<type>tanh</type>
        	<size>10</size>
        </hiddenLayer>
      </hiddenLayers>
      <outputLayer>
		<type>linear</type>
      </outputLayer>
      <bias>yes</bias>
      <learningrate>0.01</learningrate>
      <momentum>0</momentum>
    </NeuralNet>
    <thetaFile>NN1th</thetaFile>
    <resultPath>~/stage/ArmModelPython/Data/NNth/</resultPath>
  </regression>
  <costFunction>
    <gamma>0.6</gamma>
    <rho>3000</rho>
    <upsilon>1</upsilon>
  </costFunction>
  <optimisation>
  	<DDPG>
    	<maxIteration>5000</maxIteration>
    	<numberRepetition>5</numberRepetition>
    	<resultPath>~/stage/ArmModelPython/Data/DDPG/</resultPath>
  	</DDPG>
  </optimisation>
  <target>
    <sizes>
      <size>0.005</size>
      <size>0.01</size>
      <size>0.02</size>
      <size>0.04</size>
    </sizes>
    <coordinate>
      <x>0.0</x>
      <y>0.6175</y>
    </coordinate>
  </target>
  <trajectory>
    <initialPosition>PosCircu28</initialPosition>
    <maxStep>300</maxStep>
    <timeStep>0.002</timeStep>
  </trajectory>
  <kalmanFilter>10</kalmanFilter>
  <plot>
    <period>10</period>
  </plot>
</setup>
