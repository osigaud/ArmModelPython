<?xml version="1.0" encoding="UTF-8"?>
<setup>
  <data>
  	<inputSize>4</inputSize>
  	<outputSize>6</outputSize>
  	<noise>no</noise>
  </data>
  <regression>
    <NeuralNet type="simple_actor_network">
      <inputLayer>
		<type>linear</type>
      </inputLayer>
      <hiddenLayers>
        <hiddenLayer>
        	<type>tanh</type>
        	<size>200</size>
        </hiddenLayer>
        <hiddenLayer>
        	<type>tanh</type>
        	<size>200</size>
        </hiddenLayer>
      </hiddenLayers>
      <outputLayer>
		<type>linear</type>
      </outputLayer>
      <bias>yes</bias>
      <learningrate>0.0001</learningrate>
      <momentum>0</momentum>
    </NeuralNet>
    <thetaFile>NN1th</thetaFile>
    <resultPath>~/stage/ArmModelPython/Data/NNth/</resultPath>
  </regression>
  <costFunction type='CostDDPG'>
    <gamma>0.6</gamma>
    <rho>3000</rho>
    <upsilon>1</upsilon>
  </costFunction>
  <optimisation type="DDPG">
    <maxIteration>100000</maxIteration>
    <numberRepetition>1</numberRepetition>
    <resultPath>~/stage/ArmModelPython/Data/DDPG/</resultPath>
  </optimisation>
  <target>
    <sizes>
      <size>0.005</size>
      <size>0.01</size>
      <size>0.02</size>
      <size>0.04</size>
    </sizes>
    <coordinate>
      <x>0.0</x>
      <y>0.6175</y>
    </coordinate>
  </target>
  <trajectory>
    <initialPosition>PosCircu2</initialPosition>
    <maxStep>300</maxStep>
    <timeStep>0.002</timeStep>
  </trajectory>
  <estimation type='Inv'>
  	<kalmanFilter>1</kalmanFilter>
  </estimation>
  <plot>
    <period>10</period>
  </plot>
</setup>
