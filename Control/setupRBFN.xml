<?xml version="1.0" encoding="UTF-8"?>
<setup>
  <data>
  	<inputSize>4</inputSize>
  	<outputSize>6</outputSize>
  	<noise>no</noise>
  </data>
  <regression>
    <RBFN>
      <nbFeatures>3</nbFeatures>
      <lambda>0.00001</lambda>
      <fromStruct>no</fromStruct>
    </RBFN>
    <thetaFile>NNtest</thetaFile>
    <resultPath>~/ArmModelPython/Data/RBFN/</resultPath>
  </regression>
  <costFunction>
    <gamma>0.01</gamma>
    <rho>3000</rho>
    <upsilon>1</upsilon>
  </costFunction>
  <optimization type="CMAES">
    <sigma>0.01</sigma>
    <maxIteration>50000</maxIteration>
    <popsize>30</popsize>
    <numberRepetition>1</numberRepetition>
    <resultPath>~/ArmModelPython/Data/CMAES/</resultPath>
  </optimization>
  <target>
    <sizes>
      <size>0.005</size>
      <size>0.01</size>
      <size>0.02</size>
      <size>0.04</size>
    </sizes>
    <coordinate>
      <x>0.0</x>
      <y>0.6175</y>
    </coordinate>
  </target>
  <trajectory>
    <initialPosition>PosCircu28</initialPosition>
    <maxStep>300</maxStep>
    <timeStep>0.002</timeStep>
  </trajectory>
  <kalmanFilter>1</kalmanFilter>
  <plot>
    <period>10</period>
  </plot>
</setup>
