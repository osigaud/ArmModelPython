<?xml version="1.0" encoding="UTF-8"?>
<setup>
  <data>
  	<inputSize>4</inputSize>
  	<outputSize>6</outputSize>
  	<noise>yes</noise>
  </data>
  <regression>
    <NeuralNet>
      <inputLayer>
		<type>linear</type>
      </inputLayer>
      <hiddenLayers>
        <hiddenLayer>
        	<type>tanh</type>
        	<size>10</size>
        </hiddenLayer>
      </hiddenLayers>
      <outputLayer>
		<type>linear</type>
      </outputLayer>
      <bias type="yes/no">yes</bias>
      <learningrate type="float">0.0001</learningrate>
      <momentum type="float">0.</momentum>
    </NeuralNet>
    <thetaFile>NN1th</thetaFile>
    <resultPath>~/stage/ArmModelPython/Data/NNth/</resultPath>
  </regression>
  <costFunction>
    <gamma type="float">0.6</gamma>
    <rho type="float">3000</rho>
    <upsilon type="float">1</upsilon>
  </costFunction>
  <optimization type="CMAES">
    <maxIteration type="integer">3000</maxIteration>
    <numberRepetition type="integer">40</numberRepetition>
    <resultPath>~/stage/ArmModelPython/Data/CMAESK20NoPer/</resultPath>
    <sigma type="float">0.05</sigma>
    <popsize type="integer">30</popsize>
  </optimization>
  <target>
    <sizes>
      <size>0.005</size>
      <size>0.01</size>
      <size>0.02</size>
      <size>0.04</size>
    </sizes>
    <coordinate>
      <x>0.0</x>
      <y>0.6175</y>
    </coordinate>
  </target>
  <trajectory>
    <initialPosition>PosCircu15</initialPosition>
    <maxStep>300</maxStep>
    <timeStep>0.002</timeStep>
  </trajectory>
  <kalmanFilter>20</kalmanFilter>
  <plot>
    <period>10</period>
  </plot>
</setup>
